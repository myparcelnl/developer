{
  "name": "@myparcel/developer-portal",
  "version": "0.0.1",
  "private": true,
  "description": "MyParcel Developer Portal",
  "keywords": [
    "myparcel"
  ],
  "homepage": "https://developer.myparcel.nl",
  "bugs": {
    "url": "https://github.com/myparcelnl/developer/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/myparcelnl/developer.git"
  },
  "license": "MIT",
  "author": "Edie Lemoine <edie@myparcel.nl>",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "build": "yarn optimize:svg -q && vuepress build src",
    "dev": "vuepress dev src",
    "preinstall": "npx -q only-allow yarn",
    "optimize:svg": "svgo -r --multipass --final-newline --folder src/.vuepress/public",
    "prepare": "husky install",
    "preview": "npx http-server src/.vuepress/dist",
    "test": "npx concurrently yarn:test:node yarn:test:vue",
    "test:node": "vitest --config vitest-node.config.ts",
    "test:vue": "vitest --config vitest-vue.config.ts",
    "watch": "nodemon --ext ts,vue,md --watch src --exec 'vuepress build src --debug'"
  },
  "lint-staged": {
    "src/**/*.{js,ts,vue}": "eslint --fix",
    "test/**/*.{js,ts,vue}": "eslint --fix",
    "src/**/*.svg": "svgo --multipass --final-newline"
  },
  "resolutions": {
    "vite": "^2"
  },
  "dependencies": {
    "@vue/runtime-core": "^3.2.37",
    "@vuepress/plugin-docsearch": "2.0.0-beta.49",
    "@vuepress/plugin-register-components": "2.0.0-beta.49",
    "@vueuse/core": "^8.9.4",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "tailwindcss": "^3.1.6",
    "vue-recaptcha": "^2.0.2",
    "vuepress": "2.0.0-beta.49",
    "vuepress-bar": "^0.4.4"
  },
  "devDependencies": {
    "@myparcel/eslint-config": "^3.6.1",
    "@myparcel/sdk": "^2.3.1",
    "@tsconfig/node16": "^1.0.3",
    "@types/lodash-es": "^4.17.6",
    "@types/mock-fs": "^4.13.1",
    "@vitejs/plugin-vue": "^2.3.3",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/test-utils": "^2.0.2",
    "cssnano": "^5.1.12",
    "happy-dom": "^6.0.4",
    "husky": "^8.0.1",
    "lodash-unified": "^1.0.2",
    "markdown-it-multimd-table": "^4.1.3",
    "mock-fs": "^5.1.2",
    "only-allow": "^1.1.1",
    "plop": "^3.1.1",
    "rollup-plugin-visualizer": "^5.7.1",
    "svgo": "^2.8.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4",
    "vite-plugin-fonts": "^0.5.0",
    "vite-svg-loader": "3.4.0",
    "vitest": "^0.18.1"
  }
}
