  "repository": "/developer-portal",
  "scripts": {
    "build": "yarn optimize:svg -q && vuepress build src",
    "watch": "nodemon --ext ts,vue,md --watch src --exec 'vuepress build src --debug'",
    "preview": "npx http-server src/.vuepress/dist",
    "dev": "vuepress dev src",
    "test:node": "jest",
    "test:vue": "vitest",
    "test": "yarn test:node && yarn test:vue",
    "optimize:svg": "svgo -r --multipass --final-newline --folder src/.vuepress/public"
  },
  "dependencies": {
    "@vue/runtime-core": "^3.2.33",
    "@vuepress/plugin-docsearch": "2.0.0-beta.38",
    "@vuepress/plugin-register-components": "2.0.0-beta.38",
    "@vueuse/core": "^8.4.2",
    "lodash-es": "4.17.15",
    "tailwindcss": "^3.0.24",
    "vue-recaptcha": "^2.0.2",
    "vuepress": "2.0.0-beta.38",
    "vuepress-bar": "^0.4.4"
  },
  "devDependencies": {
    "@babel/core": "^7.17.10",
    "@babel/plugin-transform-runtime": "^7.17.10",
    "@babel/preset-env": "^7.17.10",
    "@babel/preset-typescript": "^7.16.7",
    "@myparcel/eslint-config": "^3.4.4",
    "@myparcel/sdk": "^2.0.0",
    "@tsconfig/recommended": "^1.0.1",
    "@types/jest": "^27.5.0",
    "@types/lodash-es": "^4.17.6",
    "@types/mock-fs": "^4.13.1",
    "@types/tailwindcss": "^3.0.10",
    "@vitejs/plugin-vue": "^2.3.3",
    "@vue/eslint-config-typescript": "^10.0.0",
    "@vue/test-utils": "^2.0.0-rc.21",
    "@vue/vue3-jest": "^27.0.0",
    "babel-jest": "^28.1.0",
    "babel-preset-vite": "^1.0.4",
    "cssnano": "^5.1.7",
    "happy-dom": "^3.1.1",
    "husky": "^7.0.4",
    "jest": "^28.1.0",
    "jest-environment-jsdom": "^28.1.0",
    "markdown-it-multimd-table": "^4.1.3",
    "mock-fs": "^5.1.2",
    "plop": "^3.1.0",
    "rollup-plugin-visualizer": "^5.6.0",
    "svgo": "^2.8.0",
    "ts-jest": "^28.0.1",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.4",
    "vite": "2.9.8",
    "vite-jest": "^0.1.4",
    "vite-svg-loader": "3.2.0",
    "vitest": "^0.10.5"
  },
  "lint-staged": {
    "src/**/*.{js,ts,vue}": "eslint --fix",
    "test/**/*.{js,ts,vue}": "eslint --fix",
    "src/**/*.svg": "svgo --multipass --final-newline"
  }
}
